
{%- assign filteredItems = "" | split: "," -%}
{%- for curricula in site.data.wai-curricula -%}
    {%- for module in curricula.modules -%}
        {% if include.data contains module.id %}
            {%- assign filteredItems = filteredItems | push: curricula.name -%}
            {%- assign filteredItems = filteredItems | push: module.name -%}
        {% endif %}    
    {%- endfor -%}
{%- endfor -%}



