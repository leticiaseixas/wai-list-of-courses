const jsonFilters = JSON.parse('[{"id":"type","name":"Type of resource","type":"checkbox","options":[{"id":"grad","name":"Graduate program"},{"id":"undergraduate","name":"Undergraduate program"},{"id":"training","name":"Training"},{"id":"certification","name":"Professional certification"},{"id":"other_type","name":"Other"}]},{"id":"audience","name":"Audience","type":"checkbox","options":[{"id":"content_author","name":"Content author"},{"id":"designer","name":"Designer"},{"id":"developer","name":"Developer"},{"id":"manager","name":"Manager"},{"id":"tester","name":"Tester"},{"id":"other_audience","name":"Other"}]},{"id":"level","name":"Level","type":"checkbox","options":[{"id":"basic","name":"Basic"},{"id":"intermediate","name":"Intermediate"},{"id":"advanced","name":"Advanced"}]},{"id":"format","name":"Format","info":"true","type":"checkbox","options":[{"id":"face-to-face","name":"Face-to-face"},{"id":"online","name":"Online"},{"id":"hybrid","name":"Hybrid"},{"id":"blended","name":"Blended"}]},{"id":"scheduling","name":"Scheduling","info":"true","type":"checkbox","options":[{"id":"scheduled","name":"Scheduled"},{"id":"unscheduled","name":"Unscheduled"}]},{"id":"cost","name":"Cost","type":"checkbox","options":[{"id":"free","name":"Free of charge"},{"id":"free_certificates_for_purchase","name":"Free with certificates for purchase"},{"id":"free_limited_time","name":"Free for limited content or duration"},{"id":"free_or_reduced_for_some","name":"Free or reduced fee for some"},{"id":"paid","name":"Paid"}]}]');
const jsonLang = JSON.parse('{"ab":{"name":"Abkhaz","nativeName":"Ğ°Ò§ÑÑƒĞ°"},"aa":{"name":"Afar","nativeName":"Afaraf"},"af":{"name":"Afrikaans","nativeName":"Afrikaans"},"ak":{"name":"Akan","nativeName":"Akan"},"sq":{"name":"Albanian","nativeName":"Shqip"},"am":{"name":"Amharic","nativeName":"áŠ áˆ›áˆ­áŠ›"},"ar":{"name":"Arabic","nativeName":"Ø§Ù„Ø¹Ø±Ø¨ÙŠØ©","rtl":true},"an":{"name":"Aragonese","nativeName":"AragonÃ©s"},"hy":{"name":"Armenian","nativeName":"Õ€Õ¡ÕµÕ¥Ö€Õ¥Õ¶"},"as":{"name":"Assamese","nativeName":"à¦…à¦¸à¦®à§€à¦¯à¦¼à¦¾"},"av":{"name":"Avaric","nativeName":"Ğ°Ğ²Ğ°Ñ€ Ğ¼Ğ°Ñ†Ó€, Ğ¼Ğ°Ğ³Ó€Ğ°Ñ€ÑƒĞ» Ğ¼Ğ°Ñ†Ó€"},"ae":{"name":"Avestan","nativeName":"avesta"},"ay":{"name":"Aymara","nativeName":"aymar aru"},"az":{"name":"Azerbaijani","nativeName":"azÉ™rbaycan dili"},"bm":{"name":"Bambara","nativeName":"bamanankan"},"ba":{"name":"Bashkir","nativeName":"Ğ±Ğ°ÑˆÒ¡Ğ¾Ñ€Ñ‚ Ñ‚ĞµĞ»Ğµ"},"eu":{"name":"Basque","nativeName":"euskara, euskera"},"be":{"name":"Belarusian","nativeName":"Ğ‘ĞµĞ»Ğ°Ñ€ÑƒÑĞºĞ°Ñ"},"bn":{"name":"Bengali","nativeName":"à¦¬à¦¾à¦‚à¦²à¦¾"},"bh":{"name":"Bihari","nativeName":"à¤­à¥‹à¤œà¤ªà¥à¤°à¥€"},"bi":{"name":"Bislama","nativeName":"Bislama"},"bs":{"name":"Bosnian","nativeName":"bosanski jezik"},"br":{"name":"Breton","nativeName":"brezhoneg"},"bg":{"name":"Bulgarian","nativeName":"Ğ±ÑŠĞ»Ğ³Ğ°Ñ€ÑĞºĞ¸ ĞµĞ·Ğ¸Ğº"},"my":{"name":"Burmese","nativeName":"á€—á€™á€¬á€…á€¬"},"ca":{"name":"Catalan","nativeName":"CatalÃ "},"ch":{"name":"Chamorro","nativeName":"Chamoru"},"ce":{"name":"Chechen","nativeName":"Ğ½Ğ¾Ñ…Ñ‡Ğ¸Ğ¹Ğ½ Ğ¼Ğ¾Ñ‚Ñ‚"},"ny":{"name":"Chichewa; Chewa; Nyanja","nativeName":"chiCheÅµa, chinyanja"},"zh":{"name":"Chinese","nativeName":"ä¸­æ–‡ (ZhÅngwÃ©n), æ±‰è¯­, æ¼¢èª"},"zh-hans":{"name":"Simplified Chinese","nativeName":"ç®€ä½“ä¸­æ–‡"},"zh-hant":{"name":"Traditional Chinese","nativeName":"ç¹é«”ä¸­æ–‡"},"cv":{"name":"Chuvash","nativeName":"Ñ‡Ó‘Ğ²Ğ°Ñˆ Ñ‡Ó—Ğ»Ñ…Ğ¸"},"kw":{"name":"Cornish","nativeName":"Kernewek"},"co":{"name":"Corsican","nativeName":"corsu, lingua c